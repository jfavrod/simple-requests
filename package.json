{
  "name": "simple-requests",
  "version": "0.2.3",
  "description": "A library for performing simple http(s) requests.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "run-s lint test clean compile",
    "clean": "rm -rf lib",
    "compile": "tsc",
    "lint": "tslint -p .",
    "push": "npm run build && npm publish",
    "test": "ts-mocha test/*.test.ts"
  },
  "keywords": [
