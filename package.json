    "HTTPS",
    "REQUEST",
    "SIMPLE",
    "GET",
    "PUT",
    "POST",
    "DELETE"
  ],
  "author": "Jason Favrod <mail@jasonfavrod.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:jfavrod/simple-requests.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "ts-mocha": "^7.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "form-data": "^3.0.0"
  }
}
